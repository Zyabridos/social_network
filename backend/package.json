{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "build": "tsc && cp server/knexfile.js dist/server/knexfile.js && cp -r server/db/migrations dist/server/db/",
    "start": "node dist/server/bin/startServer.js",
    "dev": "nodemon --watch server --exec \"ts-node server/bin/startServer.ts\"",
    "migrate": "dotenv -e .env.local -- knex migrate:latest --knexfile server/knexfile.js --env development",
    "migrate:ci": "dotenv -e .env.docker -- knex migrate:latest --knexfile server/knexfile.js --env docker",
    "setup": "npm install && npm start",
    "test": "vitest",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "type": "module",
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@types/bcrypt": "^5.0.2",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.5.0",
    "eslint": "^9.25.1",
    "eslint-define-config": "^2.1.0",
    "fastify": "^5.3.2",
    "knex": "^3.1.0",
    "objection": "^3.1.5",
    "objection-unique": "^1.2.2",
    "pg": "^8.15.5",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.0"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/fastify-cors": "^0.1.0",
    "@types/node": "^22.15.3",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "es-abstract": "^1.23.9",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2"
  }
}
