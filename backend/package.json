    "fastify": "^5.3.2",
    "knex": "^3.1.0",
    "objection": "^3.1.5",
    "pg": "^8.15.5",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.0"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
